pipeline{
    agent any
    tools{
    maven 'Maven'
    jdk 'jdk'
    }
        
        stages{
            stage('Git-Checkout')
            {
              steps{
                     git 'https://github.com/sreelakshmi665/SonarQube'
                }
            }
             stage('Clean')
        {
            steps{
              sh 'mvn -f SonarQubePipeLine clean'
            }
        }
             stage('Validate')
        {
            steps{
              sh 'mvn -f SonarQubePipeLine validate'
            }
        }
        stage('Compile')
        {
            steps{
               sh 'mvn -f SonarQubePipeLine compile'
            }
        }
             stage('Sonar Analysis'){
            steps{
                withSonarQubeEnv('sonar-server'){
                    withMaven(maven:'Maven'){
                        bat 'mvn -f SonarQubePipeLine sonar:sonar'
                    }
                }
            }

        }
        stage('Test')
        {
            steps{
                sh 'mvn -f SonarQubePipeLine test'
            }
        }
        stage('Package')
        {
            steps{
               sh 'mvn -f SonarQubePipeLine package'
            }
        }
        stage('Verify')
        {
            steps{
               sh 'mvn -f SonarQubePipeLine verify'
            }
        }
        stage('Install')
        {
            steps{
                sh 'mvn -f SonarQubePipeLine install'
            }
        }
            
            
           
        }
}
